public class App{
	public static void main(String[] args) {

		
		// .lenght =  the number of elements an array can hold, or the size of the array, is called its length
		
	    // arrays = basically used to store data
    
		String[] cars = {"AE86", "Corvette", "Camaro"};


		for(int i=0; i<cars.length; i++) {
			System.out.println(cars[i]);
		}

	}
}
