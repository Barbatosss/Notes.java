   public class App {
    public static void main(String[] args) throws Exception {

        for (int hours = 0; hours < 24; hours++){

            for(int minutes = 0; minutes<60; minutes++ ) {
                
                for(int sec = 0; sec<60; sec++ ) {
                    System.out.println(hours+":"+minutes+":"+sec);
                    
                    // can be longer
                }
                
            }

          }
      }
    }
